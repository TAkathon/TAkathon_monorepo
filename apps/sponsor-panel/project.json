{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "sponsor-panel",
  "projectType": "application",
  "sourceRoot": "apps/sponsor-panel/src",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "options": {
        "root": "apps/sponsor-panel",
        "outputPath": "dist/apps/sponsor-panel"
      }
    },
    "dev": {
      "executor": "@nx/next:server",
      "options": {
        "buildTarget": "sponsor-panel:build",
        "dev": true,
        "port": 3003
      }
    },
    "start": {
      "executor": "@nx/next:start",
      "options": {
        "root": "apps/sponsor-panel",
        "port": 3003
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["apps/sponsor-panel/**/*.{ts,tsx}"]
      }
    }
  },
  "tags": []
}